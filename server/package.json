    "sinon": "^4.4.6",
    "sinon-chai": "^3.3.0",
    "supertest": "^3.4.2",
    "unionized": "^5.0.0"
  },
  "scripts": {
    "copy-frontend-assets": "./copy-frontend-assets.sh",
    "coverage": "nyc --check-coverage --lines 50 --reporter=html --reporter=text --reporter=lcov --extension .es6 npm test",
    "createdb": "sequelize db:create",
    "dropdb": "sequelize db:drop",
    "dev": "npm run copy-frontend-assets; npm run migrate; nodemon ./src/app.es6",
    "docs": "npm run jsdoc",
    "jsdoc": "jsdoc -c .jsdoc.json -d docs/code -t ./node_modules/ink-docstrap/template -r .",
    "lint": "eslint **/**/*.es6 **/**/**/*.es6",
    "lint:fix": "eslint **/**/*.es6 **/**/**/*.es6 --fix",
    "migrate": "sequelize db:migrate:undo:all; sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "start": "node ./src/app.es6",
    "test": "npm run test:db && NODE_ENV=test mocha ./test/**/*.es6 ./test/*.es6 --exit",
    "test:db": "export NODE_ENV=test; sequelize db:drop; sequelize db:create && sequelize db:migrate",
    "undoAllSeed": "sequelize db:seed:undo:all",
    "undoAllMigrate": "sequelize db:migrate:undo:all",
    "undoLastMigrate": "sequelize db:migrate:undo",
    "devReset": "npm run dropdb; npm run createdb; npm run migrate; npm run seed; npm run copy-frontend-assets; nodemon ./src/app.es6"
  },
  "engines": {
    "node": "^10.18.0"
  },
  "nyc": {
    "extension": [
      ".es6"
    ],
    "exclude": [
      "src/pay-gov-mocks/*.*"
    ]
  }
}
