{
  "name": "frontend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "dist": "ng build --prod --env=prod --aot=false",
    "dist-trees": "ng build --env=trees --aot=false",
    "dist-staging": "ng build --prod --env=staging --aot=false",
    "dist-prod": "ng build --prod --env=prod --aot=false",
    "docs": "yarn run typedoc --out ./src/assets/typedoc --exclude '**/*.spec.ts' ./src/ --module commonjs",
    "e2e:ci": "ng e2e --port=4200 --watch=false --progress=false",
    "e2e": "ng e2e --config ./development-configurations/protractor.conf --port=4200",
    "lint": "ng lint",
    "ng": "ng",
    "pa11y": "./node_modules/pa11y-ci/bin/pa11y-ci.js",
    "serve-static-files": "./node_modules/superstatic/bin/server dist/ -c ./development-configurations/superstatic.json --port 4200",
    "build-test-pa11y": "./development-configurations/build-test-pa11y.sh",
    "snyk-test": "snyk test",
    "start": "ng serve",
    "test:ci": "ng test --sourcemaps=false --config ./development-configurations/karma.conf --single-run=true --watch=false --progress=false --code-coverage",
    "test": "ng test --config ./development-configurations/karma.conf --sourcemaps=true",
    "test:coverage": "ng test --config ./development-configurations/karma.conf --single-run=true --sourcemaps=true --code-coverage",
    "typedoc": "typedoc",
    "update-version": "node ./replace.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@types/jasminewd2": "^2.0.3",
    "angular2-csv": "^0.2.5",
    "angular2-spinner": "^1.0.10",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.14",
    "ng-in-viewport": "^1.2.0",
    "ng2-file-upload": "^1.2.1",
    "ngx-breadcrumbs": "^0.0.3",
    "ngx-md": "^3.1.1",
    "rxjs": "5.5.2",
    "uswds": "^1.4.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.2.1",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^8.0.14",
    "codelyzer": "^3.1.1",
    "emoji-regex": "^6.5.0",
    "is-docker": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pa11y-ci": "^2.0.0",
    "protractor": "^5.2.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "replace-in-file": "^2.5.2",
    "sinon": "^4.4.6",
    "snyk": "1.42.4",
    "superstatic": "^5.0.1",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typedoc": "^0.10.0",
    "typescript": "2.6.2"
  }
}
