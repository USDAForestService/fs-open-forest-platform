{
  "name": "frontend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "dist-dev": "ng build --prod --output-hashing=all --configuration=dev",
    "dist-trees": "ng build --prod --output-hashing=all --configuration=trees",
    "dist-prod": "ng build --prod --output-hashing=all --configuration=production",
    "docs": "typedoc --out ./src/assets/typedoc --exclude '**/*.spec.ts' ./src/ --module commonjs",
    "e2e:cidkr": "HEADLESS=true ng e2e --port=4200 --protractor-config ./development-configurations/protractor.conf.js --suite=docker-smoke-test",
    "e2e:su": "HEADLESS=true ng e2e --port=4200 --protractor-config ./development-configurations/protractor.conf.js --suite=su",
    "e2e": "HEADLESS=true ng e2e --protractor-config ./development-configurations/protractor.conf --port=4200",
    "e2e:cidkr:head": "ng e2e --port=4200 --protractor-config ./development-configurations/protractor.conf.js --suite=docker-smoke-test",
    "e2e:su:head": "ng e2e --port=4200  --protractor-config ./development-configurations/protractor.conf.js --suite=su",
    "e2e:head": "ng e2e --protractor-config ./development-configurations/protractor.conf --port=4200",
    "lint": "ng lint",
    "ng": "ng",
    "pa11y": "./node_modules/pa11y-ci/bin/pa11y-ci.js",
    "serve-static-files": "./node_modules/superstatic/bin/server dist/ -c ./development-configurations/superstatic.json --port 4200",
    "shutdown": "ng build --prod --configuration=shutdown",
    "build-test-pa11y": "./development-configurations/build-test-pa11y.sh",
    "start": "ng serve",
    "test:ci": "NODE_ENV=test ng test --karma-config ./development-configurations/karma.conf --code-coverage --watch=false --source-map=false --progress=false",
    "test": "NODE_ENV=test ng test --karma-config ./development-configurations/karma.conf",
    "test:coverage": "NODE_ENV=test ng test --karma-config ./development-configurations/karma.conf --watch=false --source-map --code-coverage",
    "update-version": "node ./replace.build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@arcgis/webpack-plugin": "^4.15.0",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@types/google.analytics": "0.0.39",
    "@types/jasminewd2": "^2.0.6",
    "angular2-csv": "0.2.5",
    "angular2-spinner": "^1.0.10",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.6.1",
    "esri-loader": "^2.14.0",
    "intl": "^1.2.5",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.21",
    "newrelic": "^5.10.0",
    "ng-in-viewport": "6.0.3",
    "ng2-file-upload": "^1.2.1",
    "ngx-md": "^7.1.0",
    "ngx6-angular-breadcrumbs": "^0.0.3",
    "postcss-loader": "^3.0.0",
    "rxjs": "6.4.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "tslib": "^1.9.0",
    "uswds": "^1.6.9",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.3",
    "@angular/compiler-cli": "^7.2.7",
    "@types/jasmine": "^2.8.16",
    "@types/node": "^8.10.39",
    "async": "^2.6.1",
    "codelyzer": "^4.1.1",
    "emoji-regex": "^6.5.0",
    "highlight.js": "9.13.1",
    "is-docker": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pa11y-ci": "^2.0.0",
    "protractor": "^5.4.2",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "replace-in-file": "^2.5.2",
    "sinon": "^4.4.6",
    "superstatic": "^6.0.4",
    "ts-node": "^3.1.0",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  }
}
